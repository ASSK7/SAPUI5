<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<TableSelectDialog id="idTSD" items="{M1>/employeeSet}" confirm="onConfirm" contentWidth="30%">
		<columns>
			<Column>
				<Label text="Employee ID" design="Bold"/>
			</Column>
			<Column>
				<Label text="Employee Name" design="Bold"/>
			</Column>
		</columns>
		<items>
			<ColumnListItem>
				<cells>
					<Text text="{M1>Employeid}"></Text>
					<Text text="{M1>Empname}"></Text>
				</cells>
			</ColumnListItem>
		</items>
	</TableSelectDialog>
</core:FragmentDefinition>